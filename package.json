{
  "name": "firebase-functions-es6",
  "version": "1.0.0",
  "description": "ES6 Cloud Functions for Firebase",
  "scripts": {
    "deploy": "yarn run package-functions",
    "prepackage-functions": "rimraf functions",
    "package-functions": "babel 'functionsES6' --out-dir 'functions' --presets=es2015 --copy-files --ignore 'node_modules'",
    "postpackage-functions": "cd functions && yarn"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinHwang/martinhwang-backend.git"
  },
